<%= form_for @person do |p|%>
	<%=p.label :nombre%>
	<%=p.text_field :nombre%> 
	<%=p.label :edad%>
	<%=p.text_field :edad%> 
	<%=p.label :tipo_piel%>
	<%=p.text_field :tipo_piel%> 
	<%=p.label :sensibilidad%>
	<%=p.text_field :sensibilidad%> 
	<%=p.label :poros%>
	<%=p.text_field :poros%> 
	<%=p.label :rojeces%>
	<%=p.text_field :rojeces%> 
	<%=p.label :acne%>
	<%=p.text_field :acne%>
	<%=p.label :riesgo%>
	<%=p.text_field :riesgo%>
	<%=p.submit%> 
<%end%>